{
    "title" : "SpringBoot 参考文档",
    "language" : "zh-hans",
    "style" : {
        "website": "styles/website.css"
    },
    "plugins" : [
        "github",
        "-sharing",
        "-livereload",
        "code",
        "-search", 
        "-lunr",
        "search-pro",
        "ancre-navigation",
        "flexible-alerts",
        "advanced-emoji"
    ],
    "pluginsConfig": {
        "github": {
            "url": "https://github.com/zhdbest"
        },
        "flexible-alerts" : {
            "style" : "flat",
            "tip" : {
                "className" : "tip-spring"
            },
            "note" : {
                "className" : "note-spring"
            },
            "warning" : {
                "className" : "warning-spring"
            },
            "important" : {
                "label" : "Important",
                "icon" : "fa fa-exclamation-circle",
                "className" : "important-spring"
            },
            "caution": {
                "label" : "Caution",
                "icon" : "fa fa-fire",
                "className" : "caution-spring"
            }
        }
    }
}